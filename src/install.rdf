<?xml version="1.0" encoding="utf-8"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">
<Description about="urn:mozilla:install-manifest">

    <em:id>useragentoverrider@qixinglu.com</em:id>
    <em:version>${version}</em:version>
    <em:type>2</em:type>
    <em:bootstrap>true</em:bootstrap>
    <em:unpack>${unpack}</em:unpack>
    <em:optionsURL>chrome://useragentoverrider/content/options.xul</em:optionsURL>

    <!-- Front End MetaData -->
    <em:name>User Agent Overrider</em:name>
    <em:description>Override browser User-Agent string</em:description>
    <em:creator>muzuiget</em:creator>

    <!-- Firefox -->
    <em:targetApplication>
    <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>35.0</em:minVersion>
        <em:maxVersion>36.*</em:maxVersion>
    </Description>
    </em:targetApplication>

    <!-- Localize -->
    <em:localized>
    <Description>
        <em:locale>zh-CN</em:locale>
        <em:name>User Agent Overrider</em:name>
        <em:description>修改浏览器的 User-Agent 字符串</em:description>
    </Description>
    <Description>
        <em:locale>zh-TW</em:locale>
        <em:name>User Agent Overrider</em:name>
        <em:description>修改瀏覽器的 User-Agent 字符串</em:description>
    </Description>
    <Description>
        <em:locale>es-ES</em:locale>
        <em:name>Modificador del User Agent</em:name>
        <em:description>Te permite sobrescribir el User Agent del navegador</em:description>
        <em:translator>dpzaba</em:translator>
    </Description>
    <Description>
        <em:locale>ro</em:locale>
        <em:name>User Agent Overrider</em:name>
        <em:description>Înlocuiește șirul pentru Agent-Utilizator al navigatorului</em:description>
        <em:translator>roentgen</em:translator>
    </Description>
    <Description>
        <em:locale>bg</em:locale>
        <em:name>User Agent Overrider</em:name>
        <em:description>Подмяна на User-Agent на браузъра</em:description>
        <em:translator>RealEnder</em:translator>
    </Description>
    </em:localized>

</Description>
</RDF>
