{
    "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true
    },
    "settings": {
        "ecmascript": 6
    },
    "rules": {
        "comma-dangle": [1, "always-multiline"],
        "eol-last": 1,
        "eqeqeq": 1,
        "key-spacing": [1, {"afterColon": true}],
        "no-eval": 1,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-extra-semi": 1,
        "no-mixed-spaces-and-tabs": 1,
        "no-spaced-func": 1,
        "no-trailing-spaces": 1,
        "no-undef": 1,
        "no-underscore-dangle": 0,
        "no-unused-vars": 0,
        "quotes": [1, "single", "avoid-escape"],
        "semi": [1, "always"],
        "space-after-keywords": 1,
        "space-before-blocks": 1,
        "space-before-keywords": 1,
        "space-infix-ops": 1,
        "space-in-parens": 1,
        "strict": 0
    },
    "globals": {
        "Components": true,
        "Iterator": true,
        "dump": true
    }
}
